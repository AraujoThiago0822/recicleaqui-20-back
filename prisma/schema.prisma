datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model usuarios {
  id                 Int       @id(map: "usuarios_pk") @default(autoincrement())
  email              String    @unique(map: "usuarios_unique") @db.VarChar
  senha              String    @db.VarChar
  criadoEm           DateTime  @default(now()) @db.Timestamp(6)
  editadoEm          DateTime? @db.Timestamp(6)
  tokenRedefinicao   String?   @db.VarChar
  tokenRedefGeradoEm DateTime? @db.Timestamp(6)
}
